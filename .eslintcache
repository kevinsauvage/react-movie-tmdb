ta = await response.json();\r\n      settopMovies(data.results);\r\n    }\r\n    getMovies();\r\n  }, []);\r\n\r\n  const seeAllHandler = (e) => {\r\n    const att = e.target.dataset.key;\r\n    fetchByAtt(att);\r\n  };\r\n\r\n  return (\r\n    <div className=\"content\">\r\n      <div className=\"carusel-wrapper\">\r\n        <SectionTitle title=\"Popular Movies\" />\r\n        <Carusel handleCardClickShow={handleCardClickShow} movies={popMovies} />\r\n        <div data-key=\"popular\" className=\"see-all\" onClick={seeAllHandler}>\r\n          See all\r\n        </div>\r\n      </div>\r\n\r\n      <div className=\"carusel-wrapper\">\r\n        <SectionTitle title=\"Top Rated Movies\" />\r\n        <Carusel movies={topMovies} handleCardClickShow={handleCardClickShow} />\r\n        <div data-key=\"top_rated\" className=\"see-all\" onClick={seeAllHandler}>\r\n          See all\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Content;\r\n","C:\\Users\\kevin\\code\\my-movie-tmdb\\src\\components\\main\\Search.jsx",[],"C:\\Users\\kevin\\code\\my-movie-tmdb\\src\\components\\main\\SectionTitle.jsx",[],"C:\\Users\\kevin\\code\\my-movie-tmdb\\src\\components\\main\\MovieCard.jsx",[],"C:\\Users\\kevin\\code\\my-movie-tmdb\\src\\components\\main\\Carusel.jsx",[],"C:\\Users\\kevin\\code\\my-movie-tmdb\\src\\components\\main\\YoutubeVideoPlayer.jsx",["233","234"],"import React, { useEffect, useState } from \"react\";\r\nimport ReactPlayer from \"react-player/youtube\";\r\nimport { FaSadTear, FaTimesCircle } from \"react-icons/fa\";\r\n\r\nconst YoutubePlayer = ({ singleMovie, SetVideoIsOpen }) => {\r\n  if (singleMovie.videos.results.length !== 0) {\r\n    return (\r\n      <div className=\"video-player-container\">\r\n        <ReactPlayer\r\n          url={\r\n            \"https://www.youtube.com/watch?v=\" +\r\n            singleMovie.videos.results[0].key\r\n          }\r\n          controls={true}\r\n          width=\"100%\"\r\n         